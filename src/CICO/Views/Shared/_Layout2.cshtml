@using System.Web.Mvc.Html
@model Cico.Models.Employee
<!DOCTYPE html>
   <style>
    .columnsContainer
    {
        position: relative;
        height: 400px
    }

    .leftColumn
    {
        width: 25%;
        float: left;
    }

    .rightColumn
    {
        float: right;
        width: 70%;
        margin-right: 4%;
        
    }

    .checklist{
        list-style: none;
        padding-left: 0px
        
    }
    .checkInItem {
        font-size: small
    }

    .checkInItem .ico,.description,.checkbox {
        display: inline
    }

    .float-left {
    float: left;
}

.float-right {
    float: right;
}

</style>
     
       
        <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
         <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript">
         </script>
<script type="text/javascript">
    $(function () {
        $("#infoTabs").tabs();
        $("#checkInTabs").tabs();
    });
</script>

    <body>
        <div class="page">
      
            <div id="main" >
                <div class="columnsContainer">
    <div class="rightColumn">
        <div id="infoTabs">
            <ul>
                <li><a href="#myinfo">My Info</a></li>
                <li><a href="#mydependents">My dependents</a></li>
                <li><a href="#housingquestionare">My Housing Questionaire</a></li>
            </ul>
            <div id="myinfo">
                <div class="EmployeeInfo">
                     
                   @*  @Html.Partial("~/Views/Employee/EmployeeIndex.cshtml",ViewData)*@
                </div>
            </div>
            <div id="mydependents">
                
            </div>
             <div id="housingquestionare">
                
            </div>
        </div>
    </div>
    <div class="leftColumn">
        <div id="checkInTabs">
            <ul>
                <li><a href="#checkIn">Check-In</a></li>
                <li><a href="#checkOut">Check-out</a></li>
                
            </ul>
            <div id="checkIn">
                <h4>Priority 1</h4>
                <table class="checklist">
                    
                    <thead> </thead>
                    @foreach (var checklist in Model.CheckLists) {
                        <tr>
                            <td>@checklist.Name</td>
                            <td>@checklist.DueDate</td>
                        </tr>
                    }
                    </table>
                <ul>
                <li>
   
                        <div class="checkInItem">
                            <div  class="ico ui-icon ui-icon-document">&nbsp; &nbsp;</div>
                            <div  class="checkbox"><input type="checkbox" checked/></div>
                            <div  class="description">Complete my Info</div>
                        </div>
                    </li>
                    <li>
                        <div class="checkInItem">
                            <div  class="ico ui-icon ui-icon-document">&nbsp; &nbsp;</div>
                            <div  class="checkbox"><input type="checkbox" checked/></div>
                            <div  class="description">Travel Orders</div>
                        </div>
                    </li>
                    <li>
                        <div class="checkInItem">
                            <div  class="ico ui-icon ui-icon-document">&nbsp; &nbsp;</div>
                            <div  class="checkbox"><input type="checkbox" checked/></div>
                            <div  class="description">Passport</div>
                        </div>
                    </li>
                </ul>
                <h4>Priority 2</h4>
                 <ul class="checklist">
                    <li>
                        <div class="checkInItem">
                            <div  class="ico ui-icon ui-icon-document">&nbsp; &nbsp;</div>
                            <div  class="checkbox"><input type="checkbox" checked/></div>
                            <div  class="description">Complete my Info</div>
                        </div>
                    </li>
                    <li>
                        <div class="checkInItem">
                            <div  class="ico ui-icon ui-icon-document">&nbsp; &nbsp;</div>
                            <div  class="checkbox"><input type="checkbox" checked/></div>
                            <div  class="description">Travel Orders</div>
                        </div>
                    </li>
                    <li>
                        <div class="checkInItem">
                            <div  class="ico ui-icon ui-icon-document">&nbsp; &nbsp;</div>
                            <div  class="checkbox"><input type="checkbox" checked/></div>
                            <div  class="description">Passport</div>
                        </div>
                    </li>
                </ul>
            </div>
            
            <div id="checkOut">
                
            </div>
             
        </div>
        
    </div>
    <div style="clear:both"></div>
</div>

            </div>
            <div id="footer">
            </div>
        </div>
    </body>
   
 
 


