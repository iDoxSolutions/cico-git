<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <SiteUrlToLaunchAfterPublish />
    <MSDeployServiceURL>http://ltkserver.cloudapp.net</MSDeployServiceURL>
    <DeployIisAppPath>cico</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>RemoteAgent</MSDeployPublishMethod>
    <UserName>Administrator</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="Cico.Models.SchoolContext" Order="3" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="Cico.Models.SchoolContext, ContosoUniversity" Origin="Convention" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="Cico.CICOContainer" Order="4" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="Cico.CICOContainer, ContosoUniversity" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)Cico.CICOContainer-Web.config Connection String" />
    <MSDeployParameterValue Include="$(DeployParameterPrefix)Cico.Models.SchoolContext-Web.config Connection String" />
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="Cico.CICOContainer" />
    <_ConnectionStringsToInsert Include="Cico.Models.SchoolContext" />
  </ItemGroup>
</Project>