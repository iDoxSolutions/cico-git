@model IEnumerable<Cico.Models.CheckListItemTemplate>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            Item
        </th>
        <th>
            Description
        </th>
        <th>
            Office
        </th>
        <th>
            Priority
        </th>
        <th>
            Status
        </th>
        <th>
            DueDate
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Item)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Office)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Priority)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Status)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DueDate)
        </td>
        
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CheckListItemTemplateId }) |
            @Html.ActionLink("Details", "Details", new { id=item.CheckListItemTemplateId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CheckListItemTemplateId })
        </td>
    </tr>
}

</table>
