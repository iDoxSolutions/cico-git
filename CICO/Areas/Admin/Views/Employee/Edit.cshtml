@model Cico.Areas.Admin.EmployeeModel
@{
    ViewBag.Title = "Edit Employee";
}
<h2>Edit Employee @Html.DisplayFor(m => m.Employee.FirstName) @Html.DisplayFor(m => m.Employee.LastName) </h2>
<div>

    @Html.ActionLink("Back to List", "Index")
</div>

<div class="homescreen-instructions">
    <label>Please update your information. Select "Save" to save your changes</label></div>




@using (Html.BeginForm())
{
    
    @Html.ValidationSummary(true)
    @Html.Partial("_adminEmployeeFields")
    @Html.Partial("_proxyFields")
    <input type="submit" value="Save" name="Save" />
    @Html.HiddenFor(model => model.Employee.Id)
}
<div>

    @Html.ActionLink("Back to List", "Index")
</div>
