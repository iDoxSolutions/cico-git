@model IEnumerable<Cico.Models.Employee>

@{
    ViewBag.Title = "Employee List";
}

<h2>Employee List</h2>

<p>
    @Html.ActionLink("Create New Employee", "Create")
</p>
<table>
    <tr>
        <th>
            Given Name
        </th>
       
        <th>
            Date Of Birth
        </th>

        <th>
            Title
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.GivenName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfBirth)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EmployeeId }) |
            @Html.ActionLink("Details", "Details", new { id=item.EmployeeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EmployeeId })
        </td>
    </tr>
}

</table>
<aside>
    <h3>Admin</h3>
    <p>
        Use this area to provide additional information.
    </p>
    <ul>
        <li>@Html.ActionLink("Checklist builder", "Index", "ChecklistBuilder", new { area = "Admin" }, null)</li>
        <li>@Html.ActionLink("Office", "Index", "Office", new { area = "Admin" }, null)</li>
        <li>@Html.ActionLink("Staff", "Index", "Staff", new { area = "Admin" }, null)</li>
        <li>@Html.ActionLink("Employee", "Index", "Employee", new { area = "Admin" }, null)</li>
        <li>@Html.ActionLink("Dependent", "Index", "Dependent", new { area = "Admin" }, null)</li>
        <li>@Html.ActionLink("CheckListItemTemplate", "Index", "CheckListItemTemplate", new { area = "Admin" }, null)</li>
        <li>@Html.ActionLink("CheckLists", "Index", "CheckList")</li>
    </ul>
</aside>