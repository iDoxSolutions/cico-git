<script type="text/html" id="EmergencyContact">
    <div data-bind="with: formModel">
        
        <form>
            <div data-bind="text: message,  css: messageCss "></div>
            <div data-bind="with:data">
                <div class="editor-label">
                    <label for="DateOfBirth">Emergency Contact Email Address</label>
                </div>
                <div class="editor-field">
                    <input data-bind="value: ContactEmailAddress" type="text" />
                </div>
                <div class="editor-label">
                    <label>Emergency Contact Office Phone</label>
                </div>
                <div class="editor-field">
                    <input data-bind="value: ContactOfficePhone" type="text" />
                </div>
                <div class="editor-label">
                    <label for="DateOfBirth">Emergency Contact Office Phone</label>
                </div>
                <div class="editor-field">
                    <input data-bind="value: ContactPhone"  type="text" />
                </div>
                
                <div class="editor-label">
                    <label for="DateOfBirth">Emergency Contact Phone</label>
                </div>
                <div class="editor-field">
                    <input data-bind="value: ContactRepationship" type="text" />
                </div>
            </div>
            
            <button type="button" data-bind="click: save, visible: SaveEnabled">Save</button>
        </form>
    </div>
</script>
