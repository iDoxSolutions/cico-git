<script type="text/html" id="Form2">
     <div data-bind="with:formModel">
    <form  data-bind="submit:submitForm">
        <fieldset>
            <legend>Employee</legend>

            <div class="editor-label">
                <label for="FirstName">Given Name(s)</label>
            </div>
            <div class="editor-field">
                <input class="text-box single-line" data-val="true" data-val-length="The field Given Name(s) must be a string with a maximum length of 66." data-val-length-max="66" id="FirstName" name="FirstName" type="text" value="" />
                <span class="field-validation-valid" data-valmsg-for="FirstName" data-valmsg-replace="true"></span>
            </div>
            <div class="editor-label">
                <label for="DateOfBirth">Date of Birth</label>
            </div>
            <div class="editor-field">
                <input class="text-box single-line" id="DateOfBirth" name="DateOfBirth" type="text" value="" />
                <span class="field-validation-valid" data-valmsg-for="DateOfBirth" data-valmsg-replace="true"></span>
            </div>
            <div>
                <input type="submit" value="Save"/>
            </div>
        </fieldset>
    </form>
    </div>
</script>

